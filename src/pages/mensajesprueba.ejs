<!DOCTYPE html>
<html >
<head>
    <% include ../partials/head %>
    <!-- <meta http-equiv="refresh" content="10"> -->
    
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <link href="/css/animate.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/proveedores.css" rel="stylesheet">
    <link href="/css/plugins/dataTables/datatables.min.css" rel="stylesheet" />
</head>
<body class="mini-navbar">

    <div id="wrapper">
        <% include ../partials/nav-izq %>
        <div id="page-wrapper" class="gray-bg">
            <% include ../partials/nav-top %>
            
            <!--inicia tabla-->
            <div class="wrapper wrapper-content animated fadeInRight">
                <!-- Etiquetas -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h5>Mensajes de Prueba</h5>
                                <div class="ibox-tools">
                                    <a class="collapse-link">
                                        <i class="fa fa-chevron-up"></i>
                                    </a>
                                    <a class="close-link">
                                        <i class="fa fa-times"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="ibox-content">

                                <div class="table-responsive">
                                    <table id="GridMensajesPrueba" class="table table-striped table-bordered table-hover dataTable" role="grid">
                                        <thead class="thead-dark">
                                            <tr>
                                                <th>Proveedor</th>
                                                <th>Carrier</th>
                                                <th>HoraMonitor</th>
                                                <th>Etiqueta</th>
                                                <th>TelefonoEmisor</th>
                                                <th>FechaEnvio</th>
                                                <th>FechaRecibido</th>
                                                <th>Diff_Segundos</th>
                                                <th>Diff_Minutos</th>
                                            </tr>
                                        </thead>
                                        <tbody id="datos-tabla">
                                            <!--Pinta alertas en tabla proveedores--> 
                                            <% MensajesPrueba.forEach(function(record){ %>
                                            <% if (record.Diff_Minutos == -1) { %>
                                            <tr style="background-color: Red; font-weight: bold; color: black;">
                                            <% } else { %>
                                            <tr>
                                            <% } %>
                                                <td><%= record.Proveedor %></td>
                                                <td><%= record.Carrier %></td>
                                                <td><%= record.HoraMonitor %></td>
                                                <td><%= record.Etiqueta %></td>
                                                <td><%= record.TelefonoEmisor %></td>
                                                <td><%= record.FechaEnvio.substring(0,19).replace('T',' ') %></td>
                                                <td><%= record.FechaRecibido.substring(0,19).replace('T',' ') %></td>
                                                <td><%= record.Diff_Segundos %></td>
                                                <td><%= record.Diff_Minutos %></td>
                                            </tr>
                                            <% }) %>

                                        </tbody>
                                    </table>
                                    
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                
            </div>
            <!-- termina tabla-->
            
            <% include ../partials/footer %>
        </div>
    </div>
    <% include ../partials/scripts %>
    
</body>
</html>