<!DOCTYPE html>
<html >
<head>
    <% include ../partials/head %>
   
    

</head>
<body class="mini-navbar">

    <div id="wrapper">
        <% include ../partials/nav-izq %>
        <div id="page-wrapper" class="gray-bg">
            <% include ../partials/nav-top %>
            
            <!--inicia tabla-->
            <div class="wrapper wrapper-content animated fadeInRight">
                <!-- Etiquetas -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h5>Mensajes de Prueba</h5>
                                <div class="ibox-tools">
                                    <a class="collapse-link">
                                        <i class="fa fa-chevron-up"></i>
                                    </a>
                                    <a class="close-link">
                                        <i class="fa fa-times"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="ibox-content">

                                <div class="table-responsive">
                                    <table id="GridMensajesPrueba" class="table table-striped table-bordered table-hover dataTable" role="grid">
                                        <thead class="thead-dark">
                                            <tr>
                                                <th>Proveedor</th>
                                                <th>Carrier</th>
                                                <th>HoraMonitor</th>
                                                <th>Etiqueta</th>
                                                <th>TelefonoEmisor</th>
                                                <th>HoraEnvio</th>
                                                <th>HoraRecibido</th>
                                                <th>Delay</th>
                                            </tr>
                                        </thead>
                                        <tbody id="datos-tabla">
                                            <!--Pinta alertas en tabla proveedores--> 
                                            <% MensajesPrueba.forEach(function(record){ %>
                                            <% if (Date.parse('01/01/2011 '+record.Delay)>Date.parse('01/01/2011 00:05:00')) { %>
                                            <tr style="background-color: Red; font-weight: bold; color: black;">
                                            <% } else if (Date.parse('01/01/2011 '+record.Delay)>Date.parse('01/01/2011 00:01:30')){ %>
                                            <tr style="background-color: orange; font-weight: bold; color: black;">
                                            <% } else { %>
                                            <tr>
                                            <% } %>
                                                <td><%= record.Proveedor %></td>
                                                <td><%= record.Carrier %></td>
                                                <td><%= record.HoraMonitor %></td>
                                                <td><%= record.Etiqueta %></td>
                                                <td><%= record.TelefonoEmisor %></td>
                                                <td><%= record.HoraEnvio%></td>
                                                <td><%= record.HoraRecibido %></td>
                                                <td><%= record.Delay %></td>
                                            </tr>
                                            <% }) %>

                                        </tbody>
                                    </table>
                                    
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                
            </div>
            <!-- termina tabla-->
            
            <% include ../partials/footer %>
        </div>
    </div>
    <% include ../partials/scripts %>
    
    
</body>
</html>