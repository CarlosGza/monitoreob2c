<!DOCTYPE html>
<html >
<head>
    <% include ../partials/head %>
    
    
    
</head>
<body class="mini-navbar">

    <div id="wrapper">
        <% include ../partials/nav-izq %>
        <div id="page-wrapper" class="gray-bg">
            <% include ../partials/nav-top %>

            <!--inicia tabla Programados-->
            <div class="wrapper wrapper-content animated fadeInRight">
                <!-- Etiquetas -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h5>Campa√±as</h5>
                                <div class="ibox-tools">
                                    <a class="collapse-link">
                                        <i class="fa fa-chevron-up"></i>
                                    </a>
                                    <a class="close-link">
                                        <i class="fa fa-times"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="ibox-content">

                                <div class="table-responsive">
                                    <table id="GridNoMigrados" class="table table-striped table-bordered table-hover dataTables-example">
                                        <thead class="thead-dark">
                                            <tr>
                                                <th>IdEnvio</th>
                                                <th>Cliente</th>
                                                <th>FechaCarga</th>
                                                <th>FechaInicio</th>
                                                <th>FechaFin</th>
                                                <th>Regs Pendientes</th>
                                                <th>Estatus</th>
                                                <th>Segmentado</th>
                                            </tr>
                                        </thead>
                                        <tbody id="datos-tabla">
                                            <!--Pinta alertas en tabla proveedores--> 
                                            <% MonitorNoMigrados.forEach(function(record){ %>
                                            <% if (record.Estatus === 'Enviando' ) { %>
                                            <tr style="background-color: LightGreen; font-weight: bold; color: black;">
                                            <% } else if (record.Estatus === 'Fuera de Horario') { %>
                                            <tr style="background-color: Red; font-weight: bold; color: black;">
                                            <% } else { %>
                                            <tr>
                                            <% } %>
                                                <td><%= record.idenviopr %></td>
                                                <td><%= record.cliente %></td>
                                                <td><%= record.FechaCarga.substring(0,19).replace('T',' ') %></td>
                                                <td><%= record.fechainicio.substring(0,19).replace('T',' ') %></td>
                                                <td><%= record.fechafin.substring(0,19).replace('T',' ') %></td>
                                                <td><%= record['Regs Pendientes'].toLocaleString('en') %></td>
                                                <td><%= record.Estatus %></td>
                                                <td><%= record.Segmentado %></td>
                                            </tr>
                                            <% }) %>

                                        </tbody>
                                    </table>
                                    
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                
            </div>
            <!-- termina tabla-->
            
            <% include ../partials/footer %>
        </div>
    </div>
    <% include ../partials/scripts %>
    
</body>
</html>